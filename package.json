{
  "name": "go-sim",
  "version": "1.0.2",
  "description": "Simulate the game Go in JavaScript.",
  "main": "lib/game/sim.js",
  "engines": {
    "node": "5.4.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean -s",
    "build": "babel src/ -d lib/",
    "test": "mocha --require babel-core/register -G",
    "watch": "parallelshell 'npm run watch:build' 'npm run watch:test'",
    "watch:build": "watch 'npm run build -s' src/",
    "watch:test": "npm run test -s -- -w",
    "preversion": "npm run test",
    "version": "npm run build",
    "postversion": "npm run changelog && npm run push && npm publish",
    "changelog": "echo -e \"$(changelog -m)\n\n$(cat CHANGELOG.md)\" > CHANGELOG.md && git add CHANGELOG.md",
    "push": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/renolc/go-sim.git"
  },
  "keywords": [
    "go",
    "game",
    "weiqi",
    "igo",
    "paduk",
    "baduk"
  ],
  "author": "Phillip 'renolc' Gibson <gibson.phillip.j@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/renolc/go-sim/issues"
  },
  "homepage": "https://github.com/renolc/go-sim#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.3.21",
    "babel-eslint": "^5.0.0",
    "babel-plugin-add-module-exports": "0.0.4",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "changelog": "^1.0.7",
    "growl": "^1.8.1",
    "mocha": "^2.3.4",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.4.4",
    "should": "^7.1.1",
    "watch": "^0.16.0"
  },
  "standard": {
    "globals": [
      "describe",
      "beforeEach",
      "it"
    ],
    "parser": "babel-eslint"
  }
}
